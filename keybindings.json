{
  "go.formatTool": "goimports",
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/Users/chris/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 2,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "bracketPairColorizer.depreciation-notice": false,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "go.toolsManagement.autoUpdate": true,
  "hardhat.telemetry": false,
  "boot-java.rewrite.reconcile": true,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  // for nvim4vscode
  "editor.lineNumbers": "relative",
  "vim.useSystemClipboard": true,
  "workbench.editor.enablePreview": true,
  "workbench.editor.showTabs": "single",
  "workbench.activityBar.location": "hidden",
  "editor.minimap.enabled": false,
  "java.debug.settings.showStaticVariables": false,
  "searchEverywhere.exclude": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.CVS",
    "**/.csv",
    "**/chroma_langchain_db/**",
    "**/__pycache__/**",
    "**/venv/**",
    "**/.sqlite3",
    "**/.DS_Store",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/coverage/**",
    "**/.vscode/**",
    "**/.vscode-test/**",
    "**/.history/**",
    "**/.cache/**",
    "**/.cache-loader/**",
    "**/out/**",
    "**/dist/**"
  ],
  "searchEverywhere.include": "**/*.{js,jsx,ts,tsx,java,yml,xml,json,go,py}",
  "searchEverywhere.shouldInitOnStartup": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "statusBar.background": "#3c3836",
    "statusBar.foreground": "#ebdbb2",
    "statusBar.noFolderBackground": "#3c3836",
    "statusBar.debuggingBackground": "#ff8c1a",
    "statusBar.debuggingForeground": "#fbf1c7"
  },
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "editor.tabSize": 2,
  "todo-tree.tree.groupBy": "none",
  "workbench.editor.empty.hint": "hidden",

  // vim settings
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/Users/chris/.vimrc",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "workbench.startupEditor": "newUntitledFile",
  "diffEditor.renderIndicators": false,
  "files.associations": {
    "*.tmpl": "html"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.settings.applyToAllProfiles": [],
  "editor.snippetSuggestions": "top",
  "redhat.telemetry.enabled": false,
  "java.test.config": {}
}
